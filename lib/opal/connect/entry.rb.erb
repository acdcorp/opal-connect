require 'opal/connect'

<% plugins.each do |path| %>
  require '<%= path %>'
<% end %>

options = JSON.parse(Base64.decode64('<%= client_options %>'))
options.each { |key, value| Opal::Connect.options[key] = value }

<%= entry %>

<% files.each do |file| %>
  require '<%= file %>'
<% end %>

Opal::Connect.setup
